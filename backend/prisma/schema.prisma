generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String    @id      @default(uuid())
  username  String    @unique
  email     String    @unique
  password  String?
  nickname  String?   @unique

  googleId  String?   @unique
  lineId    String?   @unique
}